            document.getElementById('employeeEmail').value = employeeEmail;
            document.getElementById('employeeDesignation').value = employeeDesignation;
            document.getElementById('currentDocument1').textContent = document1 ? document1 : 'No document uploaded';
            document.getElementById('currentDocument2').textContent = document2 ? document2 : 'No document uploaded';
            document.getElementById('currentDocument3').textContent = document3 ? document3 : 'No document uploaded';

            $('#editModal').modal('show');
        }
