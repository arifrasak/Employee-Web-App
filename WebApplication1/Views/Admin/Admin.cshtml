    <script>
        function downloadFile(fileName, employeeName) {
            fetch(`/Admin/DownloadFile?fileName=${fileName}&employeeName=${employeeName}`)
                .then(response => {
                    if (response.ok) {
                        alert("File download started!");
                        return response.blob();
                    } else {
                        alert("Error: " + response.statusText);
                        throw new Error(response.statusText);
                    }
                })
                .then(blob => {
                    const url = window.URL.createObjectURL(blob);
                    const a = document.createElement('a');
                    a.style.display = 'none';
                    a.href = url;
                    a.download = fileName;
                    document.body.appendChild(a);
                    a.click();
                    window.URL.revokeObjectURL(url);
                })
                .catch(error => {
                    alert("Error: " + error.message);
                });
        }
    </script>
    <script>
    const totalEmployees = @employeeCount;
    let count = 0;
    const duration = 9000;
    const incrementTime = 240;
    const totalIncrements = duration / incrementTime;
    const incrementAmount = Math.ceil(totalEmployees / totalIncrements);
const animateCount = () => {
        const countDisplay = document.getElementById('employeeCount');
        const interval = setInterval(() => {
            count += incrementAmount;
            if (count >= totalEmployees) {
                count = totalEmployees;
                clearInterval(interval);
            }
            countDisplay.textContent = count;
        }, incrementTime);
    };
    animateCount();
    </script>

